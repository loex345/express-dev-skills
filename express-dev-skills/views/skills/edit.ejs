<%- include('../partials/header') %>
<h1>Edit Skill #<%= skill.id %></h1>
<h1>Edit Skill Name <%= skill.skillName %></h1>
<form action="/skills/<%= skill.id %>?_method=PUT" method="POST" autocomplete="off">
    <input type="text" name="skill" value="<%= skill.skillName %>">
    <br>
    <label for="done">Confidence</label>
    <input type="number" name="confidence" id="confidence" <%= skill.confidence %>> 
    <br>
    <input type="submit" value="SAVE SKILL">
  </form>
<%- include('../partials/footer') %>